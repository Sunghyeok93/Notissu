<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/activity_with_us"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.notissu.UI.Setting.WithUs.WithUsActivity">
    <android.support.design.widget.AppBarLayout
        android:id="@+id/with_us_btn_abl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">
        <android.support.v7.widget.Toolbar
            android:id="@+id/with_us_toolbar"
            app:title="@string/setting_title_with_us"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />
    </android.support.design.widget.AppBarLayout>
    <ListView
        android:id="@+id/with_us_lv_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
